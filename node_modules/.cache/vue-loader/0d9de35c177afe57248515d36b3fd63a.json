>\n\n<script>\nexport default {\n\tname: 'Home',\n\tdata: () => ({\n\t\tregister: []\n\t}),\n\tcreated () {\n\t\tthis.getData()\n\t},\n\tcomputed: {\n\t\ttotals () {\n\t\t\tconst { register: reg } = this\n\t\t\tconst values = {\n\t\t\t\ttotalProfit: 0,\n\t\t\t\ttotalTenant: 0\n\t\t\t}\n\t\t\t//calc tot values\n\t\t\tif (reg.length) {\n\t\t\t\tvalues.totalProfit = reg.map(e => +e.value)\n\t\t\t\t\t.reduce((acc, cur) => acc + cur, 0)\n\t\t\t\t\tvalues.totalTenant = reg.length\n\t\t\t}\n\t\t\treturn values\n\t\t}\n\t},\n\tmethods: {\n\t\tgetData () {\n\t\t\tconst ref = this.$firebase.database().ref(`${window.uid}`)\n\n\t\t\tref.on('value', data => {\n\t\t\t\tconst values = data.val()\n\t\t\t\tthis.register = Object.keys(values).map(i => values[i])\n\t\t\t})\n\t\t}\n\t}\n}\n</script>\n\n<style scoped>\n.s-font {\n\tfont-size: 20pt;\n}\n.home-box {\n\tmargin-top: 40px;\n\tpadding: 10px;\n\twidth: 100%;\n\theight: calc(50vh - 30px);\n\t\t\tbackground-color: lightblue;\n\t}\n.title, .home-box {\n\tdisplay: flex;\n\tjustify-content: center;\n    flex-direction: column;\n\talign-items: center;\n}\n.title {\n\tmargin-top: 50px;\n}\n</style>\n"]}]}